{% extends 'base.html.twig' %}

{% block title %} Home {% endblock %}          

{% block stylesheets %} {% endblock %}          
{% block javascripts %} {% endblock %}          

{% block body %}

	<h3>Calculated commission</h3>

	<table class="table table-bordered table-striped">
		<thead>
			<tr>
				<th>Date</th>
				<th>User ID</th>
				<th>User Type</th>
				<th>Operation type</th>
				<th>Amount</th>
				<th>Currency</th>
				<th>Commission</th>
			</tr>
		</thead>
		<tbody>
			{% for row in data %}
			<tr>
				<td>{{ row[0] }}</td>
				<td>{{ row[1] }}</td>
				<td>{{ row[2] }}</td>
				<td>{{ row[3] }}</td>
				<td>{{ row[4] }}</td>
				<td>{{ row[5] }}</td>
				<td>{{ row[6] }}</td>
			</tr>
			{% endfor %}
		</tbody>
	</table>

{% endblock %}          